typeset -U PATH path
path=("$HOME/bin" "$HOME/.local/bin" "/usr/local/bin" "/bin" "$HOME/.gem/bin" "/usr/local/go/bin" "$HOME/go/bin" "/snap/bin" "$path[@]" "/opt/homebrew/bin")
export PATH
