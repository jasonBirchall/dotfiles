{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    }
  },
  "python.jediEnabled": true,
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.pythonPath": "/usr/bin/python3"
}
